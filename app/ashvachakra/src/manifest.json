{
    "manifest_version": 3,
    "name": "Insite",
    "version": "1.2",
    "description": "Recognize dark patterns in online stores",
    "action": {
        "default_popup": "index.html"
    },
    "permissions": [
        "activeTab",
        "tabs",
        "storage"
    ],
    "host_permissions": [
        "<all_urls>"
    ]
}